{
    "name": "olcs/olcs-cpms",
    "description": "CPMS Module",
    "type": "library",
    "license": "proprietary",
    "prefer-stable": true,
    "repositories": [
        {
            "packagist": false
        },
        {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/OLCS",
            "type": "composer"
        },
        {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/Packagist-Proxy",
            "type": "composer"
        }
    ],
    "require": {
        "elendev/nexus-composer-push": "^0.1",
        "olcs/olcs-devtools": "^4.205",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Dvsa\\Olcs\\Cpms\\": "src",
            "Dvsa\\Olcs\\Test\\": "tests"
        }
    },
    "config": {
        "secure-http": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "nexus-push": {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/olcs-cpms",
            "ignore": [
                "tests"
            ]
        }
    }
}
